extends layout
block mainContent
    include ./header.jade

    section(id="page-03")
        h1= title
        p= content
        br
        a(href='/') Page 1
        br
        a(href='/page-02') Page 2
        br
        a(href='/page-03/1') Page 3
        br
        a(href='/page-04') Page 4
        br
        br

        - var n = 0

        while n < 11
            a(href='/page-03/' + ++n)= 'Article ' + n
            br
            
    include ./footer.jade